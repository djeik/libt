local t = loadfile("libt")()

if t == nil then
  print("libt not available")
  return nil
end

local tmsg = t.require("libtmsg")
if tmsg == nil then
  print("libtmsg not availe")
  return nil
end

local test = {
    code = "print('hello from the loadstring')",
    params = {}
}

if not tmsg.send("9", test) then
  print("failed")
end
