local t = loadfile("libt")()
  or error("failed to load libt")

local tput = t.require("libtput")
  or error("failed to load libtput")

local args = { ... }

if args[1] == nil then
  print(
    "usage: ",
    shell.getRunningProgram(),
    " FILE"
  )
  return nil
end

if not tput.get(args[1]) then
  print("failed")
end

