local function plank()
  return {
    idname = 'minecraft:planks',
    data = nil,
  }
end

return {
  shape = {
    nil, plank(), nil,
    plank(), plank(), plank(),
    plank(), nil, nil
  },
  output = {
    item = {
      idname = 'IC2:itemTreetap',
      data = 0,
    },
    count = 1,
  },
}

