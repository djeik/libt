local function wing()
  return {
    idname = 'OpenBlocks:generic',
    data = 0,
  }
end

local function stick()
  return {
    idname = 'minecraft:stick',
    data = 0,
  }
end

return {
  shape = {
    wing(), stick(), wing(),
  },
  output = {
    item = {
      idname = 'OpenBlocks:hangglider',
      data = 0,
    },
    count = 1,
  },
  type = 'craft',
}

