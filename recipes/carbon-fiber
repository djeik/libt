local function dust()
  return {
    idname = 'IC2:itemDust',
    data = 2,
  }
end

return {
  shape = {
    dust(), dust(), nil,
    dust(), dust(), nil,
    nil, nil, nil,
  },
  output = {
    item = {
      idname = 'IC2:itemPartCarbonFibre',
      data = 0,
    },
    count = 1,
  },
  type = 'craft',
}

