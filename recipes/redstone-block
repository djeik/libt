local function redstone()
  return {
    idname = 'minecraft:redstone',
    data = 0,
  }
end

return {
  shape = {
    redstone(), redstone(), redstone(),
    redstone(), redstone(), redstone(),
    redstone(), redstone(), redstone(),
  },
  output = {
    item = {
      idname = 'minecraft:redstone_block',
      data = 0,
    },
    count = 1,
  },
  type = 'craft',
}

