local function fiber()
  return {
    idname = 'IC2:itemPartCarbonFibre',
    data = 0,
  }
end

return {
  shape = {
    fiber(), fiber(),
  },
  output = {
    item = {
      idname = 'IC2:itemPartCarbonMesh',
      data = 0,
    },
    count = 1,
  },
  type = 'craft',
}

