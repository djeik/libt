local function plank()
  return {
    idname = 'minecraft:planks',
    data = nil,
  }
end

return {
  shape = {
    plank(), plank(), plank(),
    plank(), nil, plank(),
    plank(), plank(), plank(),
  },
  output = {
    item = {
      idname = 'minecraft:chest',
      data = 0,
    },
    count = 1,
  },
  type = 'craft',
}

