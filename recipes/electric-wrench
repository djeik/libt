local function wrench()
  return {
    idname = 'IC2:itemToolWrench',
    data = 0,
  }
end

local function power()
  return {
    idname = 'IC2:itemRecipePart',
    data = 3,
  }
end

return {
  shape = {
    wrench(), power(),
  },
  output = {
    item = {
      idname = 'IC2:itemToolWrenchElectric',
      data = nil,
    },
    count = 1,
  },
  type = 'craft',
}

