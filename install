local function cp(f)
  if fs.exists(f) then
    print("Skipping "..f)
  else
    fs.copy("/disk"..f, f)
  end
end

cp("/lib")
cp("/libt")
cp("/tg")
cp("/tp")
cp("/tls")

shell.run("tg", "libt")
shell.run("tg", "lib/libtcho")
shell.run("tg", "lib/libtmove")
shell.run("tg", "lib/libatuin")
shell.run("tg", "lib/libtmsg")
shell.run("tg", "lib/libtput")
shell.run("tg", "lib/libvec3")
